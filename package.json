{
  "name": "keychi",
  "version": "0.0.1",
  "description": "Keychi | The best password manager for securely storing, managing data such as passwords, passkeys, OTP and credit cards.",
  "repository": "https://github.com/dynonguyen/keychi",
  "private": true,
  "type": "module",
  "workspaces": [
    "shared",
    "apps/*"
  ],
  "scripts": {
    "commit": "node ./node_modules/cz-customizable/standalone.js",
    "prepare": "husky",
    "permit-scripts": "chmod +x ./scripts/* && chmod +x ./docker/postgres/*",
    "minio:upload": "docker exec minio bash -c \"/sync/upload.sh\"",
    "minio:download": "docker exec minio bash -c \"/sync/download.sh\"",
    "docker:up": "yarn permit-scripts && ./scripts/setup.sh",
    "docker:stop": "cd ./docker && docker compose stop",
    "docker:down": "cd ./docker && docker compose down",
    "web:dev": "yarn workspace @keychi/web dev",
    "web:build": "yarn web:lint && yarn workspace @keychi/web build",
    "web:lint": "eslint \"apps/web/src\" -c ./eslint.config.mjs",
    "mobile:dev": "yarn workspace @keychi/mobile dev",
    "mobile:lint": "eslint \"apps/mobile/src\" -c ./eslint.config.mjs",
    "desktop:dev": "yarn workspace @keychi/desktop dev",
    "desktop:build": "yarn workspace @keychi/desktop build",
    "desktop:build:unpack": "yarn workspace @keychi/desktop build:unpack",
    "desktop:build:linux": "yarn workspace @keychi/desktop build:linux",
    "desktop:build:win": "yarn workspace @keychi/desktop build:win",
    "api:doc": "cd api && swag init --parseInternal=true --quiet --dir internal -g config/swagger.go -o ./docs",
    "api:dev": "yarn api:doc && cd api && gow run cmd/server/main.go",
    "api:start": "cd api && go run cmd/server/main.go",
    "api:build": "cd api && go build -o keychi-server cmd/server/main.go",
    "api:test": "cd api && go test ./..."
  },
  "dependencies": {
    "@expo/metro-config": "~0.19.4",
    "@expo/metro-runtime": "~4.0.0",
    "@expo/vector-icons": "^14.0.2",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-slot": "^1.1.1",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@tanstack/react-query": "^5.59.20",
    "await-to-js": "^3.0.0",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dompurify": "^3.1.7",
    "expo": "~52.0.19",
    "expo-blur": "~14.0.1",
    "expo-constants": "~17.0.3",
    "expo-font": "~13.0.1",
    "expo-haptics": "~14.0.0",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.13",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.6",
    "expo-web-browser": "~14.0.1",
    "framer-motion": "^11.11.8",
    "i18next": "^24.0.2",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.468.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^4.0.13",
    "react-fast-compare": "^3.2.2",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.2",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
