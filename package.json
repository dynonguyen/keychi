{
  "name": "keychi",
  "version": "0.0.1",
  "description": "Keychi | The best password manager for securely storing, managing data such as passwords, passkeys, OTP and credit cards.",
  "repository": "https://github.com/dynonguyen/keychi",
  "private": true,
  "type": "module",
  "scripts": {
    "commit": "node ./node_modules/cz-customizable/standalone.js",
    "prepare": "husky",
    "nx-reset": "nx reset",
    "permit-scripts": "chmod +x ./scripts/*",
    "minio:upload": "docker exec minio bash -c \"/sync/upload.sh\"",
    "minio:download": "docker exec minio bash -c \"/sync/download.sh\"",
    "docker:up": "yarn permit-scripts && ./scripts/setup.sh",
    "docker:stop": "cd ./docker && docker compose stop",
    "docker:down": "cd ./docker && docker compose down",
    "web:dev": "nx run web:serve",
    "web:build": "nx run web:build",
    "api:doc": "cd api && swag init --parseInternal=true --quiet --dir internal -g config/swagger.go -o ./docs",
    "api:dev": "yarn api:doc && cd api && gow run cmd/server/main.go",
    "api:start": "cd api && go run cmd/server/main.go",
    "api:test": "cd api && go test ./..."
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@nextui-org/react": "^2.4.8",
    "await-to-js": "^3.0.0",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dompurify": "^3.1.7",
    "framer-motion": "^11.11.8",
    "i18next": "^23.15.2",
    "lodash-es": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-fast-compare": "^3.2.2",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.2",
    "react-router-dom": "^6.26.2",
    "react-toastify": "^10.0.5",
    "ts-pattern": "^5.4.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@iconify-json/material-symbols": "^1.2.3",
    "@iconify/tailwind": "^1.1.3",
    "@nx/eslint": "17.2.8",
    "@nx/eslint-plugin": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/react": "17.2.8",
    "@nx/rollup": "17.2.8",
    "@nx/vite": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@swc-node/core": "^1.13.3",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/types": "^0.1.12",
    "@testing-library/react": "15.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "10.4.13",
    "cssnano": "^7.0.6",
    "cz-customizable": "^7.2.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.13.4",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "nx": "17.2.8",
    "postcss": "8.4.38",
    "prettier": "^3.3.3",
    "tailwindcss": "3.4.3",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "^5.4.5",
    "vite": "^5.0.0",
    "vite-plugin-ejs": "^1.7.0",
    "vitest": "^1.3.1"
  },
  "config": {
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  },
  "packageManager": "yarn@1.22.22",
  "nx": {}
}
