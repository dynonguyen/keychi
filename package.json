    "react-native-webview": "13.12.5",
    "react-router-dom": "^7.0.1",
    "react-toastify": "^10.0.5",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.4.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
